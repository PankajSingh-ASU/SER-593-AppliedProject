<?xml version="1.0" encoding="utf-8"?>
<asu.edu.SliderContainer
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/asu.edu"
    android:id="@+id/dateSliderContainer"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <asu.edu.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="asu.edu.labeler.YearLabeler"
        app:labelerFormat="%tY"
        app:childWidth="200dp"/>
    <asu.edu.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="asu.edu.labeler.WeekLabeler"
        app:labelerFormat="week %d"
        app:childWidth="100dp"/>
    <asu.edu.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="asu.edu.labeler.DayLabeler"
        app:labelerFormat="%tA"
        app:childWidth="150dp"/>
    <include layout="@layout/dialogbuttons"/>
</asu.edu.SliderContainer>
