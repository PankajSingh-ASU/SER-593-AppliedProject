<?xml version="1.0" encoding="utf-8"?>
<asu.edu.SliderContainer
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/asu.edu"
    android:id="@+id/dateSliderContainer"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <asu.edu.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="asu.edu.labeler.YearLabeler"
        app:labelerFormat="%tY"
        app:childWidth="180dp"/>
    <asu.edu.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="asu.edu.labeler.MonthLabeler"
        app:labelerFormat="%tB"
        app:childWidth="160dp"/>
    <asu.edu.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="asu.edu.labeler.DayDateLabeler"
        app:labelerFormat="%td %ta"
        app:childWidth="45dp"/>
    <include layout="@layout/dialogbuttons"/>
</asu.edu.SliderContainer>
